<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Công ty TNHH Gia Thái DOCTORLOAN</title>
    <link href="~/css/menu-header.min.css" rel="stylesheet" />
    <link href="~/libs/bootstrap/bootstrap.min.css" rel="stylesheet" />
    <link href="~/libs/glightbox/glightbox.min.css" rel="stylesheet" />
    <link href="~/libs/owlcarousel/owl.carousel.min.css" rel="stylesheet" />
    <link href="~/libs/swiperjs/swiper-bundle.min.css" rel="stylesheet" />
    <link href="~/libs/font-awesome_5.15.1/font-awesome_5.15.1_css_all.min.css" rel="stylesheet" />
    <link href="~/css/styles.min.css" rel="stylesheet" />
    <script src="~/libs/jquery/jquery-3.7.0.min.js"></script>
</head>
<body>
    <header>
        @await Html.PartialAsync("/Views/Common/_HeaderPartial.cshtml")
    </header>
    <main>
        @RenderBody()
    </main>
    <footer>
        @await Html.PartialAsync("/Views/Common/_FooterPartial.cshtml")
    </footer>
    <script src="~/libs/swiperjs/swiper-bundle.min.js"></script>
    <script src="~/libs/bootstrap/popper.min.js"></script>
    <script src="~/libs/bootstrap/bootstrap.min.js"></script>
    <script src="~/libs/superfish/hoverintent.js"></script>
    <script src="~/libs/superfish/superfish.min.js"></script>
    <script src="~/libs/glightbox/glightbox.min.js"></script>
    <script src="~/libs/owlcarousel/owl.carousel.min.js"></script>
    <script src="~/js/main.js"></script>
    <script src="~/js/common.js"></script>
    @await RenderSectionAsync("scripts", required: false)
</body>
</html>
