                                    sống tốt đẹp.
                                </div>
                            </div>

                        </div>

                        <div class="col-xl-6 col-12 m-auto">
                            <!-- Styles -->
                            <style>
                                #chartdiv {
                                    width: 90%;
                                    height: 500px;
                                    align-items: center;
                                }
                            </style>

                            <!-- Resources -->
                            <script src="https://cdn.amcharts.com/lib/5/index.js"></script>
                            <script src="https://cdn.amcharts.com/lib/5/map.js"></script>
                            <script src="https://cdn.amcharts.com/lib/5/geodata/worldLow.js"></script>
                            <script src="https://cdn.amcharts.com/lib/5/themes/Animated.js"></script>

                            <!-- Chart code -->
                            <script>
                                am5.ready(function () {

                                    // Create root element
                                    // https://www.amcharts.com/docs/v5/getting-started/#Root_element
                                    var root = am5.Root.new("chartdiv");


                                    // Set themes
                                    // https://www.amcharts.com/docs/v5/concepts/themes/
                                    root.setThemes([
                                        am5themes_Animated.new(root)
                                    ]);


                                    // Create the map chart
                                    // https://www.amcharts.com/docs/v5/charts/map-chart/
                                    var chart = root.container.children.push(am5map.MapChart.new(root, {
                                        panX: "rotateX",
                                        panY: "rotateY",
                                        projection: am5map.geoOrthographic()
                                    }));


                                    // Create series for background fill
                                    // https://www.amcharts.com/docs/v5/charts/map-chart/map-polygon-series/#Background_polygon
                                    var backgroundSeries = chart.series.push(
                                        am5map.MapPolygonSeries.new(root, {})
                                    );
                                    backgroundSeries.mapPolygons.template.setAll({
                                        fill: root.interfaceColors.get(),
                                        fillOpacity: 0.1,
                                        strokeOpacity: 0
                                    });
                                    backgroundSeries.data.push({
                                        geometry:
                                            am5map.getGeoRectangle(90, 180, -90, -180)
                                    });

                                    chart.animate({
                                        key: "rotationX",
                                        from: 0,
                                        to: 360,
                                        duration: 30000,
                                        loops: Infinity
                                    });

                                    // Create main polygon series for countries
                                    // https://www.amcharts.com/docs/v5/charts/map-chart/map-polygon-series/
                                    var polygonSeries = chart.series.push(am5map.MapPolygonSeries.new(root, {
                                        geoJSON: am5geodata_worldLow
                                    }));
                                    polygonSeries.mapPolygons.template.setAll({
                                        fill: root.interfaceColors.get("alternativeBackground"),
                                        fillOpacity: 0.15,
                                        strokeWidth: 0.5,
                                        stroke: root.interfaceColors.get("background")
                                    });


                                    // Create polygon series for projected circles
                                    var circleSeries = chart.series.push(am5map.MapPolygonSeries.new(root, {}));
                                    circleSeries.mapPolygons.template.setAll({
                                        templateField: "polygonTemplate",
                                        tooltipText: "{name}",
                                        interactive: true
                                    });

                                    // Define data
                                    var colors = am5.ColorSet.new(root, {});

                                    var data = [
                                        { "id": "VN", "name": "Vietnam", "value": 88791996, },
                                        { "id": "AU", "name": "Australia", "value": 22605732, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "AT", "name": "Austria", "value": 8413429, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "BD", "name": "Bangladesh", "value": 150493658, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "BY", "name": "Belarus", "value": 9559441, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "CA", "name": "Canada", "value": 34349561, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "DK", "name": "Denmark", "value": 5572594, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "EG", "name": "Egypt", "value": 82536770, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "FI", "name": "Finland", "value": 5384770, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "FR", "name": "France", "value": 63125894, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "DE", "name": "Germany", "value": 82162512, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "HU", "name": "Hungary", "value": 9966116, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "IS", "name": "Iceland", "value": 324366, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "IN", "name": "India", "value": 1241491960, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "JP", "name": "Japan", "value": 126497241, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "LA", "name": "Laos", "value": 6288037, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "LY", "name": "Libya", "value": 6422772, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "NP", "name": "Nepal", "value": 30485798, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "NL", "name": "Netherlands", "value": 16664746, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "NZ", "name": "New Zealand", "value": 4414509, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "PL", "name": "Poland", "value": 38298949, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "PT", "name": "Portugal", "value": 10689663, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "QA", "name": "Qatar", "value": 1870041, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "RO", "name": "Romania", "value": 21436495, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "RU", "name": "Russia", "value": 142835555, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "SA", "name": "Saudi Arabia", "value": 28082541, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "SE", "name": "Sweden", "value": 9440747, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "CH", "name": "Switzerland", "value": 7701690, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "SY", "name": "Syria", "value": 20766037, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "TW", "name": "Taiwan", "value": 23072000, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "TH", "name": "Thailand", "value": 69518555, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "TG", "name": "Togo", "value": 6154813, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "TN", "name": "Tunisia", "value": 10594057, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "TR", "name": "Turkey", "value": 73639596, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "UA", "name": "Ukraine", "value": 45190180, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "AE", "name": "United Arab Emirates", "value": 7890924, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "GB", "name": "United Kingdom", "value": 62417431, polygonTemplate: { fill: colors.getIndex(11) } },
                                        { "id": "US", "name": "United States", "value": 313085380, polygonTemplate: { fill: colors.getIndex(11) } },
                                    ];

                                    var valueLow = Infinity;
                                    var valueHigh = -Infinity;

                                    for (var i = 0; i < data.length; i++) {
                                        var value = data[i].value;
                                        if (value < valueLow) {
                                            valueLow = value;
                                        }
                                        if (value > valueHigh) {
                                            valueHigh = value;
                                        }
                                    }

                                    // radius in degrees
                                    var minRadius = 0.5;
                                    var maxRadius = 5;

                                    // Create circles when data for countries is fully loaded.
                                    polygonSeries.events.on("datavalidated", function () {
                                        circleSeries.data.clear();

                                        for (var i = 0; i < data.length; i++) {
                                            var dataContext = data[i];
                                            var countryDataItem = polygonSeries.getDataItemById(dataContext.id);
                                            var countryPolygon = countryDataItem.get("mapPolygon");

                                            var value = dataContext.value;

                                            var radius = minRadius + maxRadius * (value - valueLow) / (valueHigh - valueLow);

                                            if (countryPolygon) {
                                                var geometry = am5map.getGeoCircle(countryPolygon.visualCentroid(), radius);
                                                circleSeries.data.push({
                                                    name: dataContext.name,
                                                    value: dataContext.value,
                                                    polygonTemplate: dataContext.polygonTemplate,
                                                    geometry: geometry
                                                });
                                            }
                                        }
                                    })

                                    // Make stuff animate on load
                                    chart.appear(1000, 100);

                                }); // end am5.ready()
                            </script>

                            <!-- HTML -->
                            <div id="chartdiv"></div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
</section>

